{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Streamlit",
      "type": "debugpy",
      "request": "launch",
      // "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "module": "streamlit",
      "args": ["run", "${workspaceFolder}/main.py", "--", "--debug"]
    }
  ]
}
